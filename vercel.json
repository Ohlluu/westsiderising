{
  "version": 2,
  "builds": [
    {
      "src": "server-serverless.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["public/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server-serverless.js"
    }
  ],
  "functions": {
    "server-serverless.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
